# Aliases 
alias i3config='vim ~/.config/i3/config'
alias zshrc='vim ~/.zshrc'
alias zshrcp='vim ~/.zshrc-personal'
alias lunar='cd /home/merulox1 && ./LunarClient-2.10.0.AppImage'
alias betterdiscord='cd /home/merulox1 && ./BetterDiscord-Linux.AppImage'
alias waldl='~/git/waldl/waldl'
alias miru='cd ~/git/miru && npm start'
alias emacs="emacsclient -c -a 'emacs'"
alias polybar="vim ~/.config/polybar/config"
alias conkybar="vim ~/.config/i3/conky-i3statusbar"
alias wallpapers="ranger ~/.config/variety/Fetched"
alias dunstrc="vim ~/.config/dunst/dunstrc"
alias toipe="/home/merulox1/Applications/toipe -w top5000"
alias bconnect="~/.local/bin/bconnect"
alias dconnect="~/.local/bin/dconnect"
alias syu="sudo pacman -Syu"
alias s="sudo pacman -S"
alias R="sudo pacman -R"
alias Ss="sudo pacman -Ss"
alias Sy="sudo pacman -Sy"
alias conf="cd ~/.config/ && cd"
alias keyring="sudo pacman -Sy archlinux-keyring"
alias rate="xset r rate 300 25"
alias chmodall="sudo chmod 777"
alias mic="pactl load-module module-loopback"
alias umic="pactl unload-module module-loopback"
alias sddmthemes="cd /usr/share/sddm/themes"
